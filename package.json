{
  "name": "tokens_screener",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist .build",
    "lint": "tsc --noEmit && eslint . --ignore-pattern \"dist/*\" --max-warnings 0",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev": "tsup --watch --onSuccess \"node ./dist/index.js\"",
    "dev:debug": "tsup --watch --onSuccess \"node --inspect ./dist/index.js\"",
    "start": "node ./dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.13",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.13",
    "@nestjs/platform-express": "^10.4.13",
    "@nestjs/swagger": "^7.4.2",
    "@type-cacheable/core": "^14.1.0",
    "@type-cacheable/lru-cache-adapter": "^18.0.0",
    "axios": "^1.7.8",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.4.3",
    "helmet": "^8.0.0",
    "lru-cache": "^11.0.2",
    "nestjs-cls": "^4.4.1",
    "nestjs-zod": "^4.2.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "undici": "^7.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@jest/globals": "^29.7.0",
    "@nestjs/testing": "^10.4.13",
    "@swc/core": "^1.10.0",
    "@swc/jest": "^0.2.37",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "express": "^4.21.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-mock": "^29.7.0",
    "prettier": "^3.4.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  },
  "engines": {
    "node": ">=20.12.2",
    "pnpm": "~9.14.2"
  },
  "packageManager": "pnpm@~9.14.2"
